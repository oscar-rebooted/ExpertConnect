{
  "homepage": "https://oscar-rebooted.github.io/ExpertConnect/",
  "name": "expertconnect",
  "version": "1.0.0",
  "description": "A portal for consultants on commercial due diligences",
  "main": "./dist/main.js",
  "scripts": {
    "build:css": "npx tailwindcss -i ./src/styles.css -o ./public/output.css",
    "electron-prod": "npm run build && electron .",
    "electron-builder": "electron-builder",
    "start:dev": "webpack serve --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "predeploy": "npm run build",
    "postbuild": "copy dummy_data\\experts.json dist\\experts.json",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [],
  "author": "Oscar",
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "axios": "^1.7.3",
    "better-sqlite3": "^11.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.1",
    "date-fns": "^3.6.0",
    "http-server": "^14.1.1",
    "lucide-react": "^0.429.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.0.9",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "wait-on": "^8.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "css-loader": "^7.1.2",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.4.41",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "serve": "^14.2.4",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "build": {
    "appId": "com.local.expertconnect",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
